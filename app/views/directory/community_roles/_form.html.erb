<%= form_for @community_role do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_area :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :community_id %><br />
    <%= f.number_field :community_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
